<ul class="pagination float-right">
    <% if (filters.page > 1) { %>
        <li class="page-item"><a class="page-link" href="?page=1">First</a></li>
        <li class="page-item"><a class="page-link" href="?page=<%= filters.page - 1 %>">Previous</a></li>
    <% } %>
    
    <li class="page-item active"><a class="page-link" href="#"><%= filters.page %></a></li>

    <% if (filters.page < totalPages) { %>
        <li class="page-item"><a class="page-link" href="?page=<%= filters.page + 1 %>">Next</a></li>
        <li class="page-item"><a class="page-link" href="?page=<%= totalPages %>">Last</a></li>
    <% } %>
</ul>